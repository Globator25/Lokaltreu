  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "build:web": "npm -w apps/web run build",
    "build:api": "npm -w apps/api run build",
    "build": "npm run build:web && npm run build:api",
    "start:web": "node apps/web/.next/standalone/server.js",
    "start:api": "node apps/api/dist/index.js",
    "test:root": "node -e \"fs=require('fs');fs.mkdirSync('test-results/root',{recursive:true});\" && vitest run --coverage --reporter=default --reporter=junit --outputFile test-results/root/vitest-junit.xml --coverage.reporter=lcov --coverage.reporter=html",
    "test": "npm test --workspaces -- --coverage",
    "agents:sentinel:node": "node scripts/agents-sentinel-check.mjs",
    "agents:sentinels": "npm run -s agents:sentinel:node",
    "contract:check": "node scripts/contract-check.mjs",
    "test:with-stub": "node -e \"fs=require('fs');fs.mkdirSync('test-results/root',{recursive:true});\" && cross-env STUB=1 vitest run --coverage --reporter=default --reporter=junit --outputFile test-results/root/vitest-junit-stub.xml --coverage.reporter=lcov --coverage.reporter=html",
    "prepare": "husky",
    "security:api:start": "node apps/api/src/dev-server.js --port=4010 --profile=test",
    "security:api:wait": "node -e \"(async()=>{const u=process.env.API_BASE||'http://localhost:4010/health';for(let i=0;i<60;i++){try{const r=await fetch(u);if(r.ok){process.exit(0)}}catch{};await new Promise(r=>setTimeout(r,1000));}process.exit(1)})()\"",
    "test:security:anti-replay": "node scripts/security/anti-replay.mjs",
    "test:security:device-proof": "node scripts/security/device-proof.mjs",
    "test:security:plan-gate": "node scripts/security/plan-gate.mjs",
    "docs:lint": "markdownlint \"docs/0*-*.md\" \"docs/ADR/0001-single-admin-saas.md\"",
    "lint": "npm run lint:web && npm run lint:api",
    "lint:web": "npm -w apps/web run lint",
    "lint:api": "npm -w apps/api run lint"
  },
