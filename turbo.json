{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "lint": {
      "dependsOn": ["^lint"],
      "outputs": []
    },
    "typecheck": {
      "dependsOn": ["^typecheck"],
      "outputs": []
    },
    "test": {
      "dependsOn": ["^test", "typecheck"],
      "outputs": ["coverage/**", "test-results/**"],
      "env": ["CI", "STUB"]
    },
    "build": {
      "dependsOn": ["^build", "typecheck"],
      "outputs": [
        "apps/api/dist/**",
        "apps/web/.next/**",
        "packages/**/dist/**"
      ]
    },
    "dev": {
      "cache": false
    },
    "format": {
      "outputs": []
    }
  }
}
